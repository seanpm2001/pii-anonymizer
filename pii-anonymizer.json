{
  "acquire": {
    "file_path": "./test_data/test_data.csv",
    "delimiter": ","
  },
  "analyze": {
    "exclude": ["Exception"]
  },
  "anonymize": {
    "mode": "hash",
    "output_file_path": "./output",
    "output_file_format": "parquet"
  },
  "report": {
    "location": "./output",
    "level": "medium"
  }
}
