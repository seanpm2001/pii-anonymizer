{
  "acquire": {
    "file_path": "./test_data/multiple_csv/*.csv",
    "delimiter": ","
  },
  "analyze": {
    "exclude": ["Exception"]
  },
  "anonymize": {
    "mode": "replace",
    "value": "[Redacted]",
    "output_file_path": "./output",
    "output_file_name": "anonymized_output",
    "output_file_format": "csv"
  },
  "report": {
    "location": "./output",
    "level": "medium"
  }
}
